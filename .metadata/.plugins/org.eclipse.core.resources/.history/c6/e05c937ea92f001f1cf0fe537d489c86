package Interfaces;

import Modelo.TblProductocl2;
import Modelo.TblUsuariocl2;
import java.util.List;

public interface IUsuario {
	
	public void RegistrarProducto (TblUsuariocl2 producto);
	public void ActualizarProducto (TblUsuariocl2 producto);
	public void EliminarProducto (TblUsuariocl2 producto);
	public  TblProductocl2 BuscarProducto (TblUsuariocl2 producto);
	public List<TblProductocl2> ListarProducto();

}//FIN DE INTERFACE
